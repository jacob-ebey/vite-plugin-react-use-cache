{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "composite": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedDeclarations": true,
    "module": "node16",
    "noEmit": true,
    "rewriteRelativeImportExtensions": true,
    "strict": true,
    "types": ["node", "vite/client"],
    "verbatimModuleSyntax": true
  }
}
